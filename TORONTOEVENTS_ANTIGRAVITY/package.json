{
  "name": "toronto-events",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:sftp": "cross-env DEPLOY_TARGET=sftp next build",
    "build:github": "cross-env DEPLOY_TARGET=github next build",
    "fetch:github": "tsx scripts/fetch-github-data.ts",
    "build:sftp:github": "npm run fetch:github && npm run build:sftp",
    "start": "next start",
    "lint": "next lint",
    "scrape": "npx tsx scripts/run-scraper.ts",
    "deploy": "gh-pages -d build -t",
    "deploy:sftp": "npm run stocks:generate && npm run build:sftp && tsx scripts/deploy-simple.ts",
    "deploy:sftp:github": "npm run build:sftp:github && tsx scripts/deploy-simple.ts",
    "refresh:all": "tsx scripts/refresh-and-deploy.ts",
    "sync:deploy": "tsx scripts/sync-and-deploy.ts",
    "scrape:2xko": "tsx scripts/scrape-2xko-frame-data.ts",
    "update:2xko:github": "tsx scripts/update-2xko-github.ts",
    "2xko:full": "npm run scrape:2xko && npm run update:2xko:github",
    "stocks:generate": "tsx scripts/generate-daily-stocks.ts",
    "stocks:backtest": "tsx scripts/backtest-picks.ts",
    "stocks:mature": "npm run stocks:generate && npm run stocks:backtest",
    "stocks:sync": "tsx scripts/sync-to-stocksunify.ts",
    "stocks:full": "npm run stocks:mature && npm run stocks:sync",
    "stocks:v2:generate": "tsx scripts/v2/generate-ledger.ts",
    "stocks:v2:verify": "tsx scripts/v2/verify-performance.ts",
    "stocks:v2:aggregate": "tsx scripts/v2/aggregate-performance.ts",
    "stocks:v2:sync": "tsx scripts/sync-to-stocksunify2.ts",
    "stocks:v2:full": "npm run stocks:v2:generate && npm run stocks:v2:sync",
    "stocks:all": "npm run stocks:full && npm run stocks:v2:full",
    "stocks:verify": "tsx scripts/verify-picks.ts",
    "stocks:portfolio": "tsx scripts/generate-portfolio.ts",
    "findstocks:verify": "tsx scripts/verify-findstocks.ts",
    "movies:add": "node scripts/add-specific-movies.js",
    "movies:scrape": "node scripts/scrape-cineplex.js",
    "movies:discover": "node scripts/discover-trailers.js",
    "movies:update": "npm run movies:scrape && npm run movies:discover",
    "movies:bulk": "node scripts/bulk-populate-content.js",
    "movies:backup": "node scripts/backup-remote-movieshows.js",
    "movies:deploy": "node scripts/deploy-movieshows-db.js",
    "movies:test": "node tests/movieshows-puppeteer.js",
    "movies:test-db": "node tests/test-database-content.js",
    "kill-stale-node": "powershell -NoProfile -Command \"try { taskkill /F /IM node.exe 2>$null } catch {}; Write-Host 'Done (Node processes stopped if any).'\"",
    "test": "vitest"
  },
  "dependencies": {
    "axios": "^1.13.3",
    "basic-ftp": "^5.1.0",
    "cheerio": "^1.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "fake-useragent": "^1.0.1",
    "framer-motion": "^12.29.2",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "mysql2": "^3.16.3",
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "ssh2-sftp-client": "^12.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/fake-useragent": "^1.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.36.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
