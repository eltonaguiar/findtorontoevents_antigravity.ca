# Optional: set DB env so PHP getenv() sees them (Apache module).
# Rename to .htaccess and fill in; or set vars in host control panel instead.
<IfModule mod_env.c>
  SetEnv MYSQL_HOST localhost
  SetEnv MYSQL_USER ejaguiar1_favcreators
  SetEnv MYSQL_PASSWORD your_password_here
  SetEnv MYSQL_DATABASE ejaguiar1_favcreators
</IfModule>
